        data.context = getContext();
        var Context = data.context;
        if (dependcode == "" || dependcode == null || dependcode == "undefined" || dependcode == 0) {
            return false;
        }

        else {

            $.ajax({
                type: "POST",

                contentType: "application/json; charset=utf-8",
                dataType: "json",
                url: '@Url.Action("KioskSetuponchange", "KioskSetup")',
                data: JSON.stringify({
